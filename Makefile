all:
	cd asm && $(MAKE)
	cd corewar && $(MAKE)

clean:
	cd asm && $(MAKE) clean
	cd corewar && $(MAKE) clean

fclean:
	cd asm && $(MAKE) fclean
	cd corewar && $(MAKE) fclean

re:			fclean all

cdb:
	cd asm && $(MAKE) cdb
	cd corewar && $(MAKE) cdb
